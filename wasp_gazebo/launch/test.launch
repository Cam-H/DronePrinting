<?xml version="1.0"?>
<launch>

<!--   <env name="GAZEBO_PLUGIN_PATH" value="/home/cameron/PX4-Autopilot/build/px4_sitl_default/build_gazebo-classic"/> -->
  <env name="ROS_PACKAGE_PATH" value="/home/cameron/PX4-Autopilot" />
<!--    <node name="px4_setup" pkg="px4" type="$(find px4)/Tools/simulation/gazebo-classic/setup_gazebo.bash" args="$(find px4) $(find px4)/build/px4_sitl_default" />-->

  <include file="$(find px4)/launch/px4.launch"/>

  <arg name="vehicle" default="wasp"/>
  <arg name="sdf" default="$(find px4)/Tools/simulation/gazebo-classic/sitl_gazebo-classic/models/$(arg vehicle)/$(arg vehicle).sdf"/>

  <!-- gazebo configs -->
  <arg name="world" default="$(find wasp_gazebo)/worlds/wasp.world"/>
  <arg name="gui" default="true"/>
  <arg name="debug" default="false"/>
  <arg name="verbose" default="true"/>
  <arg name="paused" default="false"/>
  <arg name="respawn_gazebo" default="false"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(arg world)"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="debug" value="$(arg debug)"/>
    <arg name="verbose" value="$(arg verbose)"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="respawn_gazebo" value="$(arg respawn_gazebo)"/>
  </include>

  <!--  -->
<!--    <node name="spawner" pkg="gazebo_ros" type="spawn_model"
        args="-sdf -file $(arg sdf) -model wasp -x 0 -y 0 -z 0 -R 0 -P 0 -Y 0" respawn="false" output="screen" />-->
</launch>
